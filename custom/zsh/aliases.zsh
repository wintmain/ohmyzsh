# alias that reduce input


# >>> commands alias >>>
## my personal
# To keep progress alive when closing MAC
alias ww1="sudo pmset -a disablesleep 1"
alias ww2="sudo pmset -a disablesleep 0"
## global
alias l='ls -CFG'
alias ll="ls -alFG"
alias la="ls -aG"
alias grep='grep --color=auto'
alias p=' ps aux | grep'
alias cd=' cd'
alias ..=' cd ..; ls'
alias ...=' cd ..; cd ..; ls'
alias ....=' cd ..; cd ..; cd ..; ls'
alias cd..='..'
alias cd...='...'
alias cd....='....'
alias k9='kill -9'

# <<< commands alias <<<